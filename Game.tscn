code = "shader_type canvas_item;

uniform float brightness = 1.0;
uniform float grey_offset = 0.;

void fragment() {
    vec3 c = textureLod(SCREEN_TEXTURE, SCREEN_UV, 0.0).rgb;
	
	float avg = (c.r + c.g + c.b) / 3.0;
	avg *= brightness;
	
    c.r = mix(c.r, avg, grey_offset);
	c.g = mix(c.g, avg, grey_offset);
	c.b = mix(c.b, avg, grey_offset);

    COLOR.rgb = c.rgb;
}"

